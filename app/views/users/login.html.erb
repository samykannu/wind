<%="Login"%>
<%=form_with(url: '/users/login_create',scope: 'post') do|f|%>
<div class="form-group">
<%=f.label :email%>
<%=f.email_field :email,class: "form-control"%>
</div>
<div class="form-group">
<%=f.label :password%>
<%=f.password_field :password,class: "form-control"%>
</div>
<%=f.submit 'login'%>
<%end%>
<%=link_to"sign up",new_user_path%>
